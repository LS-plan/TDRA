.content {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.success-icon {
  font-size: 160rpx;
  color: #fa436a;
  margin-top: 100rpx;
}
.tit {
  font-size: 38rpx;
  color: #303133;
}
.btn-group {
  padding-top: 100rpx;
}
.mix-btn {
  margin-top: 30rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 600rpx;
  height: 80rpx;
  font-size: 32rpx;
  color: #fff;
  background-color: #fa436a;
  border-radius: 10rpx;
}
.mix-btn.hollow {
  background: #fff;
  color: #303133;
  border: 1px solid #ccc;
}

