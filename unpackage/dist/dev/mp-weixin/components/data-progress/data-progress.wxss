.progress .title {
  font-size: 28rpx;
  width: 170rpx;
  display: flex;
  align-items: center;
}
.progress .body {
  position: relative;
  flex: 1;
}
.progress .body .number {
  color: #fff;
  position: absolute;
  z-index: 2;
  left: 26rpx;
  height: 100%;
  display: flex;
  align-items: center;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  height: 44rpx;
}
.progress .body progress {
  padding: 6rpx 0;
}

